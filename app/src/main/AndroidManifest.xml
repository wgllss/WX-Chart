<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WXChart"
        tools:targetApi="31">
        <activity
            android:name=".activity.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.WXChart">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.LineChartActivity2"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.WXChart" />
        <activity
            android:name=".activity.LineChartActivity"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.WXChart" />
        <activity
            android:name=".activity.BarChartActivity"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.WXChart" />
        <activity
            android:name=".activity.PieChartActivity"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.WXChart" />
        <activity
            android:name=".activity.HoopChartActivity"
            android:screenOrientation="landscape"
            android:theme="@style/Theme.WXChart" />
    </application>

</manifest>